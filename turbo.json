{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": ["coverage/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**", "storybook-static/**"]
    },
    "sui-explorer#build": {
      // The Sui Explorer does not need to build dependencies first:
      "dependsOn": []
    },
    "sui-wallet#build": {
      // The Sui Explorer does not need to build dependencies first:
      "dependsOn": []
    }
  },
  "globalEnv": ["VITE_NETWORK", "VITE_VERCEL_ANALYTICS_ID"]
}
